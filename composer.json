{
    "name": "robuust/craft-commerce-coingate",
    "description": "Coingate integration for Craft Commerce",
    "version": "1.1.0",
    "type": "craft-plugin",
    "keywords": [
        "coingate",
        "commerce",
        "craftcms",
        "omnipay",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robuust",
            "homepage": "https://robuust.digital/"
        }
    ],
    "support": {
        "email": "support@robuust.digital",
        "issues": "https://github.com/robuust/craft-commerce-coingate/issues?state=open",
        "source": "https://github.com/robuust/craft-commerce-coingate",
        "docs": "https://github.com/robuust/craft-commerce-coingate",
        "rss": "https://github.com/robuust/craft-commerce-coingate/commits/master.atom"
    },
    "require": {
        "craftcms/cms": "^4.0|^5.0",
        "craftcms/commerce": "^4.0|^5.0",
        "craftcms/commerce-omnipay": "^4.1",
        "coingate/omnipay-coingate": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\coingate\\": "src/"
        }
    },
    "extra": {
        "name": "Coingate for Craft Commerce",
        "handle": "commerce-coingate"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true,
            "php-http/discovery": true
        }
    }
}
